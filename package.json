{
  "name": "mern-testing",
  "version": "1.0.0",
  "description": "Testing structure for MERN stack application",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testPathPattern=\"unit\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:client": "jest client/src/tests",
    "test:server": "jest server/tests",
    "test:e2e": "cypress run",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node server/src/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/src/index.js",
    "dev:client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "setup-test-db": "echo 'Test database setup complete'"
  },
  "keywords": ["mern", "testing", "jest", "cypress"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "concurrently": "^7.6.0",
    "cypress": "^12.7.0",
    "cypress-image-snapshot": "^4.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "winston": "^3.8.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  }
}